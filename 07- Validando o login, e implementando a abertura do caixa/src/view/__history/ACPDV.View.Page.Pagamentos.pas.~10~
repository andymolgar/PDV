{$R *.dfm}

procedure TpagePagamentos.FormShow(Sender: TObject);
var
  lFrame : TframePix;
begin
  lFrame := TframePix.Create(nil);
  lFrame.Align := alClient;
  lFrame.Parent := pnlContainerPg;
end;

